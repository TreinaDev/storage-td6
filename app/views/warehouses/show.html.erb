<p><%= Warehouse.human_attribute_name(:name) %>: </p>
<%= @warehouse.name %>
<p><%= Warehouse.human_attribute_name(:code) %>:</p>
<%= @warehouse.code %>
<p><%= Warehouse.human_attribute_name(:addresses) %>:</p>
    <% @warehouse.addresses.each do |address| %>
        <div>
            <%= address.name %>, <%= address.number %>. <%= address.district %><br>
            <%= address.city %>, <%= address.state %><br>
            <%= address.zip_code %>
            <%= address.latitude %> , <%= address.longitude %>
        </div><br>
    <% end %>
<%= link_to t('.back'), root_path%>