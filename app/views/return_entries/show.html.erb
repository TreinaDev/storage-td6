<%= @return_entry.item.code %>
<%= @return_entry.request_number %>


<% @return_entry.return_logs&.each do |log|%>
  <%= l log.date %>
  <%= log.justification %>
  <%= ReturnLog.human_enum_name(:decision, log.decision)%>
  <%log.picture.each do |picture|%>
    <%= image_tag picture%>
  <% end %>
<% end %>
<%= link_to 'Cadastrar Log de Devolução', new_return_entry_return_log_path(@return_entry) %>
<%= link_to 'Voltar', return_entries_path%>