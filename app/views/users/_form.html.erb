<% user.errors.each do |error| %>
    <span><%= error.full_message %></span><br>
<% end %>

<%= form_with model: user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :cpf %>
    <%= f.text_field :cpf %>

    <%= f.label :registration_code %>
    <%= f.text_field :registration_code %>

    <%= f.label :warehouse_id %>
    <%= f.collection_select :warehouse_id, warehouses, :id, :name, {} %>

    <%= f.submit %>
<% end %>